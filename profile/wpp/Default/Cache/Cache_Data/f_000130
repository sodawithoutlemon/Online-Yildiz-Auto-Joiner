        $(window).on("load", (function () {
        $.i18n.properties({
            name:'resources-3',
            path: '/',
            mode: 'map',
            cache: true,
            language: 'tr',
            version: '4.0.17',
            callback: function () {
                    if (document.location.search.length > 0) {
                        var path = PHI.UI.Transaction.queryString()["transaction"];
                        var variables = "";
                        if (path.split('/').length > 1) {
                            for (var i = 2; i <= path.split('/').length; i++) {
                                variables = variables == "" ? (path.split('/')[i - 1]) : (variables + ',' + path.split('/')[i - 1]);
                            }
                            path = path.split('/')[0];
                        }
                        var tmpFunc = new Function(path + ".start(" + variables + ")");
                        tmpFunc();
                    }
                    else {
                        $(document).ready(function () {
                            LMS.CORE.Cockpit.ViewCockpit.start();
                        });
                    }
                }
            });
        }));
        window.addEventListener('popstate', function (event) {
            var windowLocation = window.location.href;
            var currentState = windowLocation.substring(windowLocation.indexOf('#'));
            if (currentState.length > 1) {
                console.log(currentState);
                var path = PHI.UI.Transaction.queryString()["transaction"];
                var variables = "";
                if (path.split('/').length > 1) {
                    for (var i = 2; i <= path.split('/').length; i++) {
                        variables = variables == "" ? (path.split('/')[i - 1]) : (variables + ',' + path.split('/')[i - 1]);
                    }
                    path = path.split('/')[0];
                }
                var tmpFunc = new Function(path + ".start(" + variables + ")");
                tmpFunc();
            }
        });
    </script>
